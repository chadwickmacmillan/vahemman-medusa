    "@medusajs/medusa": "2.13.1",
    "@medusajs/medusa-js": "^6.1.10",
    "@medusajs/ui": "^4.1.1",
    "@medusajs/ui-preset": "^2.13.1",
    "@react-email/components": "^1.0.6",
    "@sanity/client": "^7.14.1",
    "avatax": "^25.12.0",
    "react-email": "^5.2.5",
    "react-router-dom": "^7.13.0",
    "resend": "^6.9.1",
