    "jest": "^30.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^7.3.1",
